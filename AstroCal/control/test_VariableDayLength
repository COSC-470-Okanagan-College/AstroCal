import unittest

from control import getWhenSolEclipseLoc

class TestDayLength(unittest.TestCase):
    def test_DayLength_WhenSuccessful(self):
        self.assertEqual(getVariableDayLength(2022,10,4,1),((11,21)),"Test Fail")

    def test_SolEcl_WhenUnsuccessful(self):
        self.assertNotEqual(getVariableDayLength(2022,9,27,1),(()),"Test Fail")


if __name__ == '__main__':
    unittest.main()